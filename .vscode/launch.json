{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Go",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${fileDirname}",
            "env": {},
            "args": []
        },
        {
            "type": "java",
            "name": "Java",
            "request": "launch",
            "cwd": "${fileDirname}",
            "mainClass": "${file}",
            "console": "internalConsole",
            "args": ""
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch TypeScript File",
            "program": "${file}",
            "preLaunchTask": "tsc: build - tsconfig.json"
        }
    ]
}